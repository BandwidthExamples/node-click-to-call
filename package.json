{
	"name": "node-click-to-call",
	"version": "2.0.0",
	"description": "Click-to-call application",
	"repository": "https://github.com/BandwidthExamples/node-click-to-call",
	"author": "avbel",
	"license": "MIT",
	"main": "lib/index.js",
	"scripts": {
		"start": "node -e \"require('./lib/index')().then(app => {app.listen(process.env.PORT || 3000); console.log('Ready');});\"",
		"lint": "xo",
		"test": "ava",
		"dev:frontend-server": "webpack-dev-server -d -w",
		"dev:server": "cross-env PORT=5000 npm start",
		"dev": "npm-run-all --parallel dev:*",
		"webpack": "webpack -d",
		"build": "webpack -p --env=prod",
		"heroku-postbuild": "yarn run build"
	},
	"dependencies": {
		"@bandwidth/node-bandwidth-extra": "^1.0.1",
		"bcryptjs": "^2.4.3",
		"debug": "^2.6.8",
		"dotenv": "^4.0.0",
		"koa": "^2.3.0",
		"koa-body": "^2.3.0",
		"koa-passport": "^3.0.0",
		"koa-router": "^7.2.1",
		"koa-session": "^5.4.0",
		"koa-static": "^4.0.1",
		"moment": "^2.18.1",
		"mongoose": "^4.10.0",
		"node-bandwidth": "^2.3.3",
		"nodemailer": "^4.0.1",
		"passport-local": "^1.0.0",
		"redis": "^2.8.0",
		"sse_stream": "^0.0.2",
		"vue": "^2.3.4",
		"vue-router": "^2.7.0",
		"vue2-timepicker": "^0.1.4",
		"vuejs-datepicker": "^0.8.4"
	},
	"devDependencies": {
		"ava": "^0.19.1",
		"babel-core": "^6.25.0",
		"babel-loader": "^7.1.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.5.2",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"cross-env": "^5.0.1",
		"css-loader": "^0.28.4",
		"html-webpack-plugin": "^2.29.0",
		"nodeenv": "^0.2.1",
		"npm-run-all": "^4.0.2",
		"supertest": "^3.0.0",
		"testdouble": "^2.1.2",
		"vue-loader": "^13.0.0",
		"vue-template-compiler": "^2.3.4",
		"webpack": "2.2",
		"webpack-dev-server": "^2.5.0",
		"xo": "^0.18.2"
	},
	"engines": {
		"node": ">=8.0"
	},
	"xo": {
		"esnext": true,
		"envs": [
			"node"
		],
		"ignores": [
			"assets/**",
			"public/**"
		]
	}
}
